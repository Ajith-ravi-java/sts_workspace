
public class AmstrangExample {

	public static void main(String[] args) {

		int number=123;

		int gettingNumber=0;
		
		int multipleNumber=0;

		for(int i=0;i<3;i++) {
			gettingNumber=number%10;
			for(int j=0;j<gettingNumber;j++) {
				multipleNumber*=gettingNumber;
			}
		}
		System.out.println(multipleNumber);
	}

}
