
public class FactorialExample {
	
	public static void main(String[] args) {
		
//		int num=4;
//		int multiple=1;
//		
//		for(int i=1;i<=num;i++) {
//			multiple*=i;
//		}
//		System.out.println(multiple);
		System.out.println(factorialOf());
	}
	
	public static int  factorialOf() {
		int num=4;
		int multiple=1;
		
		for(int i=1;i<=num;i++) {
			multiple*=i;
		}
		return multiple;
	}
	

}
